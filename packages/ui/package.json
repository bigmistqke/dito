{
  "name": "@ditojs/ui",
  "version": "0.272.0",
  "description": "Dito.js UI is a simple UI library, built with Vue.js for use in Dito.js Admin and elsewhere",
  "main": "dist/dito-ui.umd.min.js",
  "repository": "https://github.com/ditojs/dito/tree/master/packages/ui",
  "author": "JÃ¼rg Lehni <juerg@scratchdisk.com> (http://scratchdisk.com)",
  "license": "MIT",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build:icons": "yarn inline-assets src/icons/*.svg --template src/icons/template.js > src/styles/mixins/_icons.sass",
    "build:vue": "vue-cli-service build --target lib --name dito-ui src/index.js",
    "build": "yarn build:icons && yarn build:vue",
    "watch": "yarn build:vue --watch",
    "prepare": "yarn build"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11",
    "ie_mob >= 11"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-inset": {}
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@ditojs/utils": "^0.272.0",
    "@soda/get-current-script": "^1.0.2",
    "core-js": "^3.18.1",
    "postcss-inset": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@ditojs/babel-preset": "^0.272.0",
    "@ditojs/build": "^0.272.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "autoprefixer": "^9.8.7",
    "babel-loader": "^8.2.2",
    "fibers": "^5.0.0",
    "postcss": "^8.3.8",
    "postcss-loader": "^4.3.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "1.32.13",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  }
}
