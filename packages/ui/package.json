{
  "name": "@ditojs/ui",
  "version": "0.274.0",
  "description": "Dito.js UI is a simple UI library, built with Vue.js for use in Dito.js Admin and elsewhere",
  "main": "dist/dito-ui.umd.min.js",
  "repository": "https://github.com/ditojs/dito/tree/master/packages/ui",
  "author": "JÃ¼rg Lehni <juerg@scratchdisk.com> (http://scratchdisk.com)",
  "license": "MIT",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build:icons": "yarn inline-assets src/icons/*.svg --template src/icons/template.js > src/styles/mixins/_icons.sass",
    "build:vue": "vue-cli-service build --target lib --name dito-ui --no-module src/index.js",
    "build": "yarn build:icons && yarn build:vue",
    "watch": "yarn build:vue --watch",
    "prepare": "yarn build"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11",
    "ie_mob >= 11"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-inset": {}
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@ditojs/utils": "^0.274.0",
    "@soda/get-current-script": "^1.0.2",
    "core-js": "^3.21.1",
    "postcss-inset": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@ditojs/babel-preset": "^0.274.0",
    "@ditojs/build": "^0.274.0",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "os-browserify": "^0.3.0",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "1.49.9",
    "sass-loader": "^12.6.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.70.0"
  }
}
